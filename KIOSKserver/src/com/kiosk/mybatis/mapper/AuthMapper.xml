<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Auth">
	<select id="select" parameterType="int" resultType="Auth">
		select * from auth where auth_id =#{auth_id}
	</select>
	
</mapper>